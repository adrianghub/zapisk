{
  "name": "zapiski",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "pnpm seed && concurrently \"pnpm start-server\" \"pnpm dev\"",
    "start-server": "tsc-watch --noClear --onSuccess \"node ./dist/server.js\"",
    "seed": "tsc && node ./dist/seed.js",
    "docker-up": "docker-compose up -d",
    "docker-down": "docker-compose down"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.4",
    "@types/pg": "^8.10.9",
    "@types/quill": "^2.0.14",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "quill": "^1.3.7",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "tsc-watch": "^6.0.4"
  }
}
